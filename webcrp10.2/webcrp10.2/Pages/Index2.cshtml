                <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/js/bootstrap-datepicker.min.js"></script>
                <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.9.0/locales/bootstrap-datepicker.ru.min.js"></script>
                <script>
                    $(document).ready(function () {
                        var carModels = {
                            "Toyota": [
                                { ID_avto: "Camry AT", id: 1105 },
                            ],
                            "Hyundai": [
                                { ID_avto: "Staria 3,5 AT Luxe", id: 1101 },
                                { ID_avto: "Creta 2,0 AT", id: 1103 },
                                { ID_avto: "Creta 1,6 AT", id: 1104 },
                                { ID_avto: "Solaris II AT NEW", id: 1108 },
                                { ID_avto: "Solaris II AT", id: 1109 },
                                { ID_avto: "Solaris 1,6 AT", id: 1110 },
                                { ID_avto: "Solaris 1,6 MT", id: 1111 },
                                { ID_avto: "Solaris 1,4 MT", id: 1112 },
                                { ID_avto: "Accent МТ", id: 1117 }
                            ],
                            "Lada": [
                                { ID_avto: "Granta liftback AT NEW", id: 1114 },
                                { ID_avto: "Granta sedan MT NEW", id: 1115 },
                                { ID_avto: "Granta liftback MT", id: 1116 },
                                { ID_avto: "Kalina universal MT", id: 1118 }
                            ],
                            "Kia": [
                                { ID_avto: "Rio AT NEW", id: 1107 },
                                { ID_avto: "Sportage NEW AT", id: 1102 },
                                { ID_avto: "Rio X-Line AT NEW", id: 1106 }
                            ],
                            "Renault": [
                                { ID_avto: "Logan 2 MT", id: 1113 }
                            ]
                        };

                        function updateModelList(mark) {
                            var ID_avtoSelect = $('#ID_avto');
                            ID_avtoSelect.empty().append('<option value="">Выберите модель</option>');
                            if (carModels[mark]) {
                                carModels[mark].forEach(function (ID_avto) {
                                    ID_avtoSelect.append(new Option(ID_avto.ID_avto, ID_avto.id));
                                });
                            }
                        }

                        function updateCatalogItems(mark, selectedID) {
                            if (selectedID) {
                                $('.catalog_item').hide();
                                $('.catalog_item').filter(function () {
                                    return $(this).find('a.modal-phone-call').data('id') == selectedID;
                                }).show();
                            } else if (mark) {
                                $('.catalog_item').hide();
                                $('.catalog_item').filter(function () {
                                    return $(this).find('h4').text().includes(mark);
                                }).show();
                            } else {
                                $('.catalog_item').show();
                            }
                        }

                        $('#mark').on('change', function () {
                            var mark = this.value;
                            updateModelList(mark);
                            updateCatalogItems(mark, null);
                        });

                        $('#ID_avto').on('change', function () {
                            var selectedID = $(this).val();
                            var mark = $('#mark').val();
                            updateCatalogItems(mark, selectedID);
                        });
                    });
                </script>
              
             

              
            }
            <script>
                document.addEventListener('DOMContentLoaded', function () {
                    document.querySelectorAll('.add-to-favorites').forEach(function (button) {
                        button.addEventListener('click', function (event) {
                            event.preventDefault();

                            let item = event.target.closest('.catalog_item');
                            let itemId = item.getAttribute('id');
                            let itemContent = item.outerHTML;

                            // Получаем ID текущего пользователя
                            let userId = 'current_user_id'; // Замените на реальный идентификатор пользователя

                            // Получаем текущие избранные элементы из localStorage
                            let allFavorites = JSON.parse(localStorage.getItem('favorites')) || {};

                            // Добавляем или обновляем избранные элементы для текущего пользователя
                            if (!allFavorites[userId]) {
                                allFavorites[userId] = {};
                            }
                            allFavorites[userId][itemId] = itemContent;

                            // Сохраняем обновленные избранные элементы в localStorage
                            localStorage.setItem('favorites', JSON.stringify(allFavorites));

                            // Отображаем сообщение
                            let messageElement = document.getElementById('message');
                            messageElement.style.display = 'block';

                            // Скрываем сообщение через 3 секунды
                            setTimeout(function () {
                                messageElement.style.display = 'none';
                            }, 3000); // 3000 миллисекунд = 3 секунды
                        });
                    });
                });
            </script>
            <script>
                document.addEventListener('DOMContentLoaded', function () {
                    var params = new URLSearchParams(window.location.search);
                    if (params.has('orderSubmitted')) {
                        var message = document.getElementById('success-message');
                        message.style.display = 'block';
                        setTimeout(function () {
                            message.style.display = 'none';
                        }, 3000); // Скрываем через 3 секунды
                    }
                });
            </script>

        </div>
    </div>

</body>

